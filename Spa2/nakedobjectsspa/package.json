{
  "name": "nakedobjects.spa",
  "version": "10.2.0",
  "description": "Single Page Application client for a Naked Objects application.",
  "license": "Apache-2.0",
  "author": "Richard Pawson <rpawson@nakedobjects.org>",
  "bugs": "https://github.com/NakedObjectsGroup/NakedObjectsFramework/issues",
  "homepage": "https://github.com/NakedObjectsGroup/NakedObjectsFramework",
  "repository": {
    "type": "git",
    "url": "git://github.com/NakedObjectsGroup/NakedObjectsFramework.git"
  },
  "keywords": [
    "rest",
    "restful",
    "objects",
    "naked",
    "nakedobjects",
    "ddd",
    "domain",
    "driven",
    "design",
    "api"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepackage": "node prepackage.js",
    "compile:restful-objects": "npm i && rimraf dist/nakedobjects-restful-objects && ng-packagr -p restful-objects/package.json",
    "pack:restful-objects": "cd dist/nakedobjects-restful-objects && npm pack && cd ../..",
    "install:restful-objects": "npm i dist/nakedobjects-restful-objects/nakedobjects-restful-objects-10.2.0.tgz",
    "build:restful-objects": "yarn compile:restful-objects && yarn pack:restful-objects && yarn install:restful-objects",
    "compile:services": "npm i && rimraf dist/nakedobjects-services && ng-packagr -p services/package.json",
    "pack:services": "cd dist/nakedobjects-services && npm pack && cd ../..",
    "install:services": "npm i dist/nakedobjects-services/nakedobjects-services-10.2.0.tgz",
    "build:services": "yarn compile:services && yarn pack:services && yarn install:services",
    "compile:view-models": "npm i && rimraf dist/nakedobjects-view-models && ng-packagr -p view-models/package.json",
    "pack:view-models": "cd dist/nakedobjects-view-models && npm pack && cd ../..",
    "install:view-models": "npm i dist/nakedobjects-view-models/nakedobjects-view-models-10.2.0.tgz",
    "build:view-models": "yarn compile:view-models && yarn pack:view-models && yarn install:view-models",
    "compile:gemini": "npm i && rimraf dist/nakedobjects-gemini && ng-packagr -p gemini/package.json",
    "pack:gemini": "cd dist/nakedobjects-gemini && npm pack && cd ../..",
    "install:gemini": "npm i dist/nakedobjects-gemini/nakedobjects-gemini-10.2.0.tgz",
    "build:gemini": "yarn compile:gemini && yarn pack:gemini && yarn install:gemini",
    "compile:cicero": "npm i && rimraf dist/nakedobjects-cicero && ng-packagr -p cicero/package.json",
    "pack:cicero": "cd dist/nakedobjects-cicero && npm pack && cd ../..",
    "install:cicero": "npm i dist/nakedobjects-cicero/nakedobjects-cicero-10.2.0.tgz",
    "build:cicero": "npm i && yarn compile:cicero && yarn pack:cicero && yarn install:cicero",
    "build:clean": "rimraf dist && rimraf node_modules/@nakedobjects",
    "build:libs": "yarn build:restful-objects && yarn build:services && yarn build:view-models && yarn build:gemini && yarn build:cicero",
    "build:app": "ng build packages --no-progress"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.11",
    "@angular/cdk": "~9.2.4",
    "@angular/common": "~9.1.11",
    "@angular/compiler": "~9.1.11",
    "@angular/core": "~9.1.11",
    "@angular/forms": "~9.1.11",
    "@angular/platform-browser": "~9.1.11",
    "@angular/platform-browser-dynamic": "~9.1.11",
    "@angular/router": "~9.1.11",
    "auth0-js": "^9.13.3",
    "lodash": "~4.17.11",
    "lodash-es": "~4.17.11",
    "moment": "~2.24.0",
    "rxjs": "~6.6.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.10",
    "@angular/cli": "~9.1.10",
    "@angular/compiler-cli": "~9.1.11",
    "@angular/language-service": "~9.1.11",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "~4.14.121",
    "@types/lodash-es": "~4.17.2",
    "@types/node": "~14.0.9",
    "codelyzer": "^5.0.1",
    "cpr": "^3.0.1",
    "find-in-files": "^0.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "9.1.5",
    "protractor": "~5.4.0",
    "replace-in-file": "^3.4.2",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.0",
    "typescript": "~3.8.3"
  }
}
