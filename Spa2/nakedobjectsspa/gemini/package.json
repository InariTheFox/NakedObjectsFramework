{
    "ngPackage": {
        "$schema": "./node_modules/ng-packagr/package.schema.json",
        "lib": {
            "entryFile": "src/public_api.ts",
            "umdModuleIds": {
                "lodash": "_",
                "moment": "moment",
                "moment-timezone": "moment-timezone",
                "lodash-es/difference": "difference",
                "lodash-es/findIndex": "findIndex",
                "lodash-es/first": "first",
                "lodash-es/every": "every",
                "lodash-es/keys": "keys",
                "lodash-es/omit": "omit",
                "lodash-es/filter": "filter",
                "lodash-es/flatten": "flatten",
                "lodash-es/mapValues": "mapValues",
                "lodash-es/zipObject": "zipObject",
                "lodash-es/each": "each",
                "lodash-es/find": "find",
                "lodash-es/forEach": "forEach",
                "lodash-es/map": "map",
                "lodash-es/some": "some",
                "lodash-es/concat": "concat",
                "@nakedobjects/view-models": "@nakedobjects/view-models",
                "@nakedobjects/services": "@nakedobjects/services",
                "@nakedobjects/restful-objects": "@nakedobjects/restful-objects"
            }
        },
        "whitelistedNonPeerDependencies": [
            "auth0-js",
            "lodash",
            "lodash-es",
            "moment",
            "@angular/cdk",
            "@nakedobjects/restful-objects",
            "@nakedobjects/services",
            "@nakedobjects/view-models"
        ],
        "dest": "../dist/nakedobjects-gemini"
    },
    "name": "@nakedobjects/gemini",
    "version": "10.0.1",
    "description": "Single Page Application client for a Naked Objects application.",
    "license": "Apache-2.0",
    "author": "Richard Pawson <rpawson@nakedobjects.org>",
    "bugs": "https://github.com/NakedObjectsGroup/NakedObjectsFramework/issues",
    "homepage": "https://github.com/NakedObjectsGroup/NakedObjectsFramework",
    "repository": {
        "type": "git",
        "url": "git://github.com/NakedObjectsGroup/NakedObjectsFramework.git"
    },
    "angular-cli": {},
    "keywords": [
        "rest",
        "restful",
        "objects",
        "naked",
        "nakedobjects",
        "ddd",
        "domain",
        "driven",
        "design",
        "api"
    ],
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "dependencies": {
        "auth0-js": "^9.13.3",
        "lodash": "~4.17.11",
        "lodash-es": "~4.17.11",
        "moment": "2.22.2",
        "@angular/cdk": "^7.3.0",
        "@nakedobjects/restful-objects": "^10.0.1",
        "@nakedobjects/services": "^10.0.1",
        "@nakedobjects/view-models": "^10.0.1"
    },
    "peerDependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "core-js": "^2.5.4",
        "rxjs": "~6.3.3",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular/cli": "~7.2.0",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/jasmine": "~3.3.0",
        "@types/jasminewd2": "~2.0.6",
        "@types/lodash": "~4.14.121",
        "@types/lodash-es": "~4.17.2",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.1.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
